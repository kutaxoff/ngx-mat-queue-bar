{
    "extends": "../../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "qb",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "qb",
            "kebab-case"
        ],
        "no-host-metadata-property": false,
        "no-redundant-jsdoc": false,
        "interface-over-type-literal": false
    }
}
